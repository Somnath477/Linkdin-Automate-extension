{
  "manifest_version": 3,
  "name": "LinkedIn Connect Automator",
  "version": "1.0",
  "description": "Automate connection requests on LinkedIn using wxt.dev and React.",
  
  "host_permissions": ["https://www.linkedin.com/search/results/people/"],
  
  "action": {
    "default_popup": "index.html",
    "default_title": "LinkedIn Connect Automator",
    "default_icon": {
      "16": "logo192.png",
      "128": "logo512.png"
    }
  }
}
